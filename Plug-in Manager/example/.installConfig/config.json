{
  "software_name": "# 示例软件名称，配合版本号在系统内查找插件安装路径",
  "software_version": "1.0.0# 若没有版本号，则代表适用于全版本",
  "plugin_name": "示例插件名称 # 若没有名称，则使用文件夹路径作为名称",
  "plugin_version": "0.1.0",
  "installation_method #如果没有此项，则使用copy方式复制所有文件到软件名的默认插件路径下，路径会记录在脚本的config.json中": {
    "copy1": {
      "type": "copy",
      "source_path": "/path/to/plugin",
      "target_path": "/path/to/software/plugins"
    },
    "copy2": {
        "type": "copy",
        "source_path": "/path/to/plugin",
        "target_path": "/path/to/software/plugins"
    },
    "script1": {
      "type": "script",
      "script_path": "/path/to/installation/script.bat"
    }
  }
}
